# 1) Espacio muestral: los posibles resultados de la instalación
espacio_muestral <- c("CSE",   # Instala correctamente y sin errores
"IA",    # Instala con advertencias (soporte menor)
"FI")    # Falla en la instalación
# 2) Frecuencias observadas (número de clientes)
frecuencias <- c(12, 5, 3)
# 3) Probabilidades (frecuencias relativas)
probabilidades <- frecuencias / sum(frecuencias)
# 4) Tabla de resumen con probabilidades
resumen <- data.frame(
Evento       = espacio_muestral,
Frecuencia   = frecuencias,
Probabilidad = round(probabilidades, 2)
)
print(resumen)
# 5) Complementos de cada probabilidad: P(no Evento) = 1 – P(Evento)
complementos <- 1 - probabilidades
resumen$Complemento <- round(complementos, 2)
print(resumen)
# 6) Definición de umbrales para “alta” y “baja”
umbral_alta <- 0.95
umbral_baja <- 0.05
# 7) Comprobaciones de probabilidad “alta” o “baja”
alta  <- probabilidades >= umbral_alta
baja  <- probabilidades <= umbral_baja
# 8) Mensajes según significancia para cada evento
for (i in seq_along(espacio_muestral)) {
evento <- espacio_muestral[i]
p      <- probabilidades[i]
if (alta[i]) {
cat(sprintf("P(%s) = %.2f → Probabilidad SIGNIFICATIVAMENTE ALTA\n",
evento, p))
} else if (baja[i]) {
cat(sprintf("P(%s) = %.2f → Probabilidad SIGNIFICATIVAMENTE BAJA\n",
evento, p))
} else {
cat(sprintf("P(%s) = %.2f → Probabilidad moderada\n",
evento, p))
}
}
# 9) (Opcional) Extraer P(CSE) y P(FI) con ifs individuales
P_CSE <- probabilidades[espacio_muestral == "CSE"]
P_FI  <- probabilidades[espacio_muestral == "FI"]
if (P_CSE >= umbral_alta) {
print("CSE tiene probabilidad significativamente alta")
} else {
print("CSE NO tiene probabilidad significativamente alta")
}
if (P_FI <= umbral_baja) {
print("FI tiene probabilidad significativamente baja")
} else {
print("FI NO tiene probabilidad significativamente baja")
}
